{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "always"],
        "quotemark": [true, "single", "avoid-template"],
        "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
        "no-console": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}